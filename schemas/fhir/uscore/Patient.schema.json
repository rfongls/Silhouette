{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "USCore Patient (minimal)",
  "type": "object",
  "required": ["resourceType", "name"],
  "properties": {
    "resourceType": { "const": "Patient" },
    "identifier": { "type": "array" },
    "name": {
      "type": "array",
      "items": { "type": "object" }
    },
    "birthDate": { "type": "string" }
  }
}
